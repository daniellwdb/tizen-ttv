{
  "name": "tizen-ttv",
  "version": "0.0.1",
  "private": true,
  "description": "Twitch app for TizenBrew with support for emotes.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "esbuild ./src/index.ts --bundle --minify --outfile=./dist/index.js",
    "start": "node --experimental-strip-types ./src/index.ts"
  },
  "dependencies": {
    "emotettv": "^1.0.2"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "esbuild": "0.25.5",
    "typescript": "^5.8.3"
  },
  "appName": "TizenTTV",
  "keys": [
    "MediaPlayPause",
    "MediaPlay",
    "MediaPause",
    "MediaStop",
    "MediaFastForward",
    "MediaRewind",
    "MediaTrackNext",
    "MediaTrackPrevious"
  ],
  "packageType": "mods",
  "websiteURL": "https://hisense.tv.twitch.tv/"
}
