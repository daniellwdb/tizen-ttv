{
  "name": "tizen-ttv",
  "version": "0.0.1",
  "private": true,
  "description": "Twitch app for TizenBrew with support for emotes.",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "start": "node --experimental-strip-types ./src/index.ts"
  },
  "dependencies": {
    "emotettv": "^1.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tsconfig/strictest": "^2.0.5",
    "rollup": "^4.42.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "appName": "TizenTTV",
  "keys": [
    "MediaPlayPause",
    "MediaPlay",
    "MediaPause",
    "MediaStop",
    "MediaFastForward",
    "MediaRewind",
    "MediaTrackNext",
    "MediaTrackPrevious"
  ],
  "packageType": "mods",
  "websiteURL": "https://hisense.tv.twitch.tv/"
}
